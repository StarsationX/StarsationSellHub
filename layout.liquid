<div class="dark bg-background text-white relative !scroll-smooth font-geist">
  <!-- HeaderBar -->
  <div id="headerBar" class="mx-auto w-full sticky top-0 z-50 backdrop-blur-md bg-white/[0.05]">
    <nav class="mx-auto max-w-[1120px] flex flex-row justify-between items-center px-4 py-4">
      <a href="/">
        <div class="flex flex-row items-center gap-2 w-fit md:min-w-[203px]">
          <fade-in-image alt="website logo" src="{{ image }}"
            class="h-8 w-8 rounded-full object-contain"></fade-in-image>
          <span class="text-xl font-bold hidden sm:inline-block">
            {{ name }}
          </span>
        </div>
      </a>
      <div class="hidden flex-row items-center gap-1 md:flex">
        <link-button href="/tos" class="text-xs hover:text-[--accent-color]" data-id="000001" data-label="Terms"
          data-variant="ghost" data-href="">Terms</link-button>

        <link-button href="/reviews" class="text-xs hover:text-[--accent-color]" data-id="000002" data-label="Reviews"
          data-variant="ghost" data-href="">Reviews</link-button>
      </div>
      <div class="flex flex-row items-center gap-4">
        <currency-selector class="bg-white/10 hover:bg-white/25 border-transparent"></currency-selector>
        <a href="/payment"
          class="flex duration-500 justify-center items-center gap-3 rounded-md text-xl px-3 py-2.5 bg-white/10 hover:bg-white/25 relative">
