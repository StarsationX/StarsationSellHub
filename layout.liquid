  <script id="header-script">
    // Select the navbar
    const navbar = document.getElementById('headerBar');

    // Function to check scroll position and update navbar background
    function updateNavbarBackground() {
      if (window.scrollY === 0) {
        // At the top: make background transparent
        navbar.classList.remove('backdrop-blur-md', 'bg-white/[0.05]');
        navbar.classList.add('bg-transparent');
      } else {
        // Scrolled down: restore original background
        navbar.classList.remove('bg-transparent');
        navbar.classList.add('backdrop-blur-md', 'bg-white/[0.05]');
      }
    }

    // Listen for scroll events
    window.addEventListener('scroll', updateNavbarBackground);

    // Call once on load to set the correct initial state
    updateNavbarBackground();
  </script>
