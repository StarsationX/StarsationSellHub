            stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
            class="lucide lucide-shopping-cart">
            <circle cx="8" cy="21" r="1" />
            <circle cx="19" cy="21" r="1" />
            <path d="M2.05 2.05h2l2.66 12.42a2 2 0 0 0 2 1.58h9.78a2 2 0 0 0 1.95-1.57l1.65-7.43H5.12" />
          </svg>
          <span id="cart-count"
            class="h-4 w-4 bg-[--accent-color] text-xs rounded-full aspect-square absolute top-0 right-0 translate-x-1/3 -translate-y-1/3 flex items-center justify-center text-white leading-none">
            {{ cartItems.items.length | plus: cartItems.bundles.length }}
